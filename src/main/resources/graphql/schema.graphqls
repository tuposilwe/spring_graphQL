type Player{
   id:ID!
   name:String
   team:Team
}

enum Team{
    ManCity,
    Chelsea,
    ManU,
    LiverPool
}

type Query{
    findAll:[Player]
    findOne(id:ID): Player
}

type Mutation{
   create(name:String, team:Team): Player
   update(id:ID,name:String, team:Team): Player
   delete(id:ID): Player
}